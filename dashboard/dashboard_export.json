{
  "dashboard_title": "RoboVM Network Analytics",
  "slug": "robovm-network-analytics",
  "description": "Comprehensive monitoring dashboard for RoboVM decentralized robot network",
  "css": "",
  "dashboard_position": "{}",
  "published": true,
  "charts": [
    {
      "id": 1,
      "slice_name": "Active Robots Overview",
      "viz_type": "big_number_total",
      "datasource": "robots",
      "params": {
        "metric": "COUNT(*)",
        "adhoc_filters": [
          {
            "clause": "WHERE",
            "expressionType": "SQL",
            "sqlExpression": "status = 'active'"
          }
        ]
      },
      "position": {
        "x": 0,
        "y": 0,
        "w": 3,
        "h": 4
      }
    },
    {
      "id": 2,
      "slice_name": "Pending Tasks",
      "viz_type": "big_number_total",
      "datasource": "tasks",
      "params": {
        "metric": "COUNT(*)",
        "adhoc_filters": [
          {
            "clause": "WHERE",
            "expressionType": "SQL",
            "sqlExpression": "status = 'pending'"
          }
        ]
      },
      "position": {
        "x": 3,
        "y": 0,
        "w": 3,
        "h": 4
      }
    },
    {
      "id": 3,
      "slice_name": "Total Tasks Completed",
      "viz_type": "big_number_total",
      "datasource": "tasks",
      "params": {
        "metric": "COUNT(*)",
        "adhoc_filters": [
          {
            "clause": "WHERE",
            "expressionType": "SQL",
            "sqlExpression": "status = 'completed'"
          }
        ]
      },
      "position": {
        "x": 6,
        "y": 0,
        "w": 3,
        "h": 4
      }
    },
    {
      "id": 4,
      "slice_name": "Total Rewards Distributed",
      "viz_type": "big_number_total",
      "datasource": "tasks",
      "params": {
        "metric": "SUM(reward)",
        "number_format": "SMART_NUMBER",
        "adhoc_filters": [
          {
            "clause": "WHERE",
            "expressionType": "SQL",
            "sqlExpression": "status = 'completed'"
          }
        ]
      },
      "position": {
        "x": 9,
        "y": 0,
        "w": 3,
        "h": 4
      }
    },
    {
      "id": 5,
      "slice_name": "Task Completion Trend",
      "viz_type": "line",
      "datasource": "tasks",
      "params": {
        "metrics": ["COUNT(*)"],
        "groupby": ["DATE(completed_at)"],
        "time_range": "Last 7 days",
        "x_axis_format": "smart_date",
        "y_axis_format": "SMART_NUMBER"
      },
      "position": {
        "x": 0,
        "y": 4,
